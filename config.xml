<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "Gff"
        versionCode = "10"
        version     = "1.0.0">

    <name>Tadapp.DEMO</name>

    <description>
        Fgb
    </description>

    <author>Tadapp</author>

    <plugin name='cordova-plugin-splashscreen' />
    <plugin name='cordova-plugin-inappbrowser' />
    <plugin name='cordova-plugin-network-information' />

    <preference name='phonegap-version' value='cli-9.0.0' />

    <gap:plugin name='onesignal-cordova-plugin' spec='2.4.5' source='npm' />

    <preference name='android-build-tool' value='gradle' />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name='orientation' value='portrait' />

    <preference name="SplashMaintainAspectRatio" value="false" />
    <preference name="SplashScreenSpinnerColor" value="#ffffff" />

    <preference name="Fullscreen" value="false" />


    <platform name="android">
        <icon src="res/android/icon/ldpi_1572924466_8520c6744c48b03b2ae5.png" density="ldpi" />
        <icon src="res/android/icon/mdpi_1572924466_8520c6744c48b03b2ae5.png" density="mdpi" />
        <icon src="res/android/icon/hdpi_1572924466_8520c6744c48b03b2ae5.png" density="hdpi" />
        <icon src="res/android/icon/xhdpi_1572924466_8520c6744c48b03b2ae5.png" density="xhdpi" />
        <icon src="res/android/icon/xxhdpi_1572924466_8520c6744c48b03b2ae5.png" density="xxhdpi" />
        <icon src="res/android/icon/xxxhdpi_1572924466_8520c6744c48b03b2ae5.png" density="xxxhdpi" />

        <splash src="res/android/screen/1572924517_807aaa50973f235bc85a.png" density="port-hdpi"/>
        <splash src="res/android/screen/1572924517_dc00631ad9a3e9a4997c.png" density="port-ldpi"/>
        <splash src="res/android/screen/1572924517_114bbc41ad8554857d47.png" density="port-mdpi"/>
        <splash src="res/android/screen/1572924517_6ca6e60d5a1cb763fb29.png" density="port-xhdpi"/>
        <splash src="res/android/screen/1572924517_0701e4b7d1312c291b7d.png" density="land-hdpi"/>
        <splash src="res/android/screen/1572924517_37cfe8d761dccf5569bf.png" density="land-ldpi"/>
        <splash src="res/android/screen/1572924517_0afe044ff93b0ca5286f.png" density="land-mdpi"/>
        <splash src="res/android/screen/1572924517_9387648ffcf7f0554f3c.png" density="land-xhdpi"/>
    </platform>

    <access origin='*'/>
    <plugin name='cordova-plugin-whitelist' />
    <allow-intent href='http://*/*'/>
    <allow-intent href='https://*/*'/>
    <allow-intent href='tel:*'/>
    <allow-intent href='sms:*'/>
    <allow-intent href='mailto:*'/>
    <allow-intent href='geo:*'/>

</widget>